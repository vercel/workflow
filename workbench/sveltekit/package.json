{
  "name": "@workflow/example-sveltekit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "start": "vite preview --port 3000",
    "prepare": "svelte-kit sync || echo ''",
