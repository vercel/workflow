    "svelte-check": "^4.3.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-devtools-json": "^1.0.0"
  },
  "dependencies": {
    "@ai-sdk/react": "2.0.76",
    "@node-rs/xxhash": "1.7.6",
    "@opentelemetry/api": "^1.9.0",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/adapter-vercel": "^6.1.1",
    "@vercel/otel": "^1.13.0",
    "@workflow/ai": "workspace:*",
    "ai": "catalog:",
