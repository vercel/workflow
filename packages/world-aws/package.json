{
  "name": "@workflow/world-aws",
  "version": "4.1.0-beta.1",
  "description": "A reference World implementation based on AWS",
  "main": "./dist/world/index.js",
  "types": "./dist/world/index.d.ts",
  "bin": {
    "aws-workflow": "./dist/bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/world/index.d.ts",
      "default": "./dist/world/index.js"
    },
    "./cdk": {
      "types": "./dist/cdk/index.d.ts",
      "default": "./dist/cdk/index.js"
    }
  },
  "files": [
    "dist",
    "scripts",
    "lib",
    "lambda",
    "world",
    "src",
    "tsconfig.json",
    "cdk.json",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "tsc --build --clean && rm -rf dist",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "bootstrap": "./scripts/bootstrap.sh",
    "deploy": "./scripts/deploy.sh",
    "teardown": "./scripts/teardown.sh",
    "outputs": "./scripts/outputs.sh"
  },
  "keywords": [
    "workflow",
    "workflow-devkit",
    "aws",
    "lambda",
    "serverless",
    "durable-execution",
    "orchestration",
    "state-machine",
    "dynamodb",
    "sqs",
    "s3",
    "nextjs",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.921.0",
    "@aws-sdk/client-s3": "^3.921.0",
    "@aws-sdk/client-sqs": "^3.921.0",
    "@aws-sdk/lib-dynamodb": "^3.921.0",
    "@vercel/queue": "0.0.0-alpha.28",
    "@workflow/errors": "4.0.1-beta.2",
    "@workflow/world": "4.0.1-beta.3",
    "@workflow/world-local": "4.0.1-beta.3",
    "aws-cdk-lib": "^2.221.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "constructs": "^10.4.2",
    "ms": "^2.1.3",
    "ora": "^9.0.0",
    "source-map-support": "^0.5.21",
    "ulid": "^3.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@workflow/tsconfig": "workspace:*",
    "esbuild": "^0.25.11",
    "typescript": "^5.9.3"
  }
}
